<Window x:Class="gittest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:gittest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="450">
    <StackPanel>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Button Grid.Column="0" Margin="0 20 0 0" Content="one"/>
            <Button Grid.Column="1" Margin="0 20 0 0" Content="two"/>
            <TextBlock Text="hello whatsup..." Grid.Column="2" Margin="10"/>
        </Grid>
        <CheckBox Content="helllllllo" Margin="0 20 0 0" Padding="2" />
        <CheckBox Content="seeeeeee you" />
        <CheckBox Content=" another test " />
    </StackPanel>
</Window>
